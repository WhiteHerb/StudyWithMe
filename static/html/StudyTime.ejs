    <section class="u-align-center u-clearfix u-palette-1-light-3 u-section-1" id="sec-1f05">
      <div class="u-clearfix u-sheet u-valign-middle u-sheet-1">
        <h2 class="u-text u-text-default u-text-palette-1-base u-text-1">Study Time</h2>
        <p class="u-text u-text-default u-text-palette-1-base u-text-1"> 자신의 순공시간을 기록해보세요</p>
        <div class="u-expanded-width u-list u-list-1">
          <div class="u-repeater u-repeater-1">
            <% membersTime.forEach(key => { %>
            <div class="u-container-style u-list-item u-repeater-item">
              <div class="u-container-layout u-similar-container u-container-layout-2">
                <div class="u-border-8 u-border-palette-1-light-2 u-line u-line-horizontal u-line-2">
                </div>
                <h4 class="u-text u-text-default u-text-palette-1-base u-text-4"><%= Object.keys(key)[0] %></h4>
                
                <p class="u-text u-text-grey-50 u-text-5"> <%= key[Object.keys(key)[0]][1] %>시간 <%= key[Object.keys(key)[0]][2] %>분 </p>
              </div>
            </div>
            <% }) %>
          
          </div>
        </div>
      </div>
      <form action="/studytime/uploadtime" method="post">
        <input type="number" name="timeH" id="timeH"> 시간
        <input type="number" name="timeM" id="timeM"> 분
        
        <br />
        <% if(islogin){ %>
        <input type="submit" value="등록하기">
        <% }else{ %>
        <a href="/kakologin/authorize">
          <img src="https://developers.kakao.com/tool/resource/static/img/button/login/full/ko/kakao_login_medium_narrow.png">
        </a>
        <% } %>
      </form>
      <script src="/js/writeTime.js"></script>
    </section>
    <br>
    <footer class="u-align-center u-clearfix u-footer u-grey-80 u-footer" id="sec-3e8e"><div class="u-clearfix u-sheet u-sheet-1">
